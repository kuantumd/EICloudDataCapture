                    });
                  
                }
                else {
                   
                    if (loggedInUserId != data.LastActiveUserId) {
                        UnLockRecordForEdit(obj.id, false);
                    }
                    else
                    {
                        UnLockRecordForEdit(obj.id, true);

                    }

                }
              
            }
        }

        function errorFunc() {
            alert('error');
        }


